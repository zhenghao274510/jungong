<view class="content-warpe data-v-2b8075de"><block wx:if="{{addressData.id!=''}}"><view class="data-v-2b8075de"><view class="address-tit data-v-2b8075de">收货人信息</view><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="address-section data-v-2b8075de" bindtap="__e"><view class="order-content data-v-2b8075de"><view class="cen data-v-2b8075de"><view class="top uni-flex data-v-2b8075de"><view class="mobile data-v-2b8075de"><text class="data-v-2b8075de">{{addressData.name}}</text><text style="margin-left:15px;" class="data-v-2b8075de">{{addressData.phone}}</text></view></view><text class="cell-tit data-v-2b8075de">{{addressData.district+addressData.location}}</text></view><view class="uni-icon uni-icon-arrowright data-v-2b8075de"></view></view></view></view></block><block wx:else><view data-event-opts="{{[['tap',[['choseAddress',['$event']]]]]}}" class="noAddress data-v-2b8075de" bindtap="__e"><view class="mobile address-tit data-v-2b8075de">选择您的收货地址</view><view class="uni-icon uni-icon-arrowright data-v-2b8075de"></view></view></block><view class="goods-section data-v-2b8075de"><view class="address-tit data-v-2b8075de">商品信息</view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block class="data-v-2b8075de"><view class="uni-flex d-items-center cart-item-top data-v-2b8075de"><image class="shop-icon data-v-2b8075de" src="/static/img/shop.png"></image>{{item.shopName+''}}</view><block wx:for="{{item.cartList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="g-item data-v-2b8075de"><image class="product-img data-v-2b8075de" src="{{v.goodsImage}}"></image><view class="right data-v-2b8075de"><text class="title clamp data-v-2b8075de">{{v.goodsName}}</text><block wx:if="{{v.goodsSpecName!=''}}"><view style="margin:5px 0;" class="data-v-2b8075de"><text class="spec uni-ellipsis data-v-2b8075de">{{v.goodsSpecName}}</text></view></block><view class="price-box data-v-2b8075de"><text class="price data-v-2b8075de">{{"￥"+v.goodsPrice}}</text><text class="number data-v-2b8075de">{{"x"+v.count}}</text></view></view></view></block><view class="uni-list-cell data-v-2b8075de"><text class="cell-tip data-v-2b8075de">商品金额</text><view class="cell-tit data-v-2b8075de">{{"￥"+item.shopPrice}}</view></view><block wx:if="{{freight!=0}}"><view class="uni-list-cell data-v-2b8075de"><text class="cell-tip data-v-2b8075de">运费</text><view class="cell-tit data-v-2b8075de">{{"￥"+freight}}</view></view></block><view class="uni-list-cell data-v-2b8075de"><text class="cell-tip data-v-2b8075de">共计</text><view class="cell-tit data-v-2b8075de">{{'￥'+item.totalPrice+''}}</view></view></block></block></view><block wx:if="{{complinePrice!=0}}"><view class="uni-list-cell data-v-2b8075de"><text class="cell-tip data-v-2b8075de">积分抵扣</text><view class="cell-tit data-v-2b8075de">{{"-￥"+complinePrice}}</view></view></block><view class="uni-list-cell data-v-2b8075de"><text class="cell-tip data-v-2b8075de">备注</text><input class="input data-v-2b8075de" type="text" value="{{remarks}}" placeholder="请输入你的留言" data-event-opts="{{[['input',[['__set_model',['','remarks','$event',[]]]]]]}}" bindinput="__e"/></view><view class="footer data-v-2b8075de"><view class="price-content data-v-2b8075de"><block wx:if="{{endPrice!=0}}"><text class="price-tip data-v-2b8075de">￥</text></block><text class="price data-v-2b8075de">{{endPrice}}</text></view><text class="submit data-v-2b8075de" hover-class="btn-hover" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交订单</text></view></view>